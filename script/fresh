#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  if [[ "$($shome/script/version | jq -r '.consul_template')" != "$CONSUL_TEMPLATE_VERSION" ]]; then
    return 1
  fi

  if [[ "$($shome/script/version | jq -r '.confd')" != "$CONFD_VERSION" ]]; then
    return 1
  fi

  return 0
}

source sub "$BASH_SOURCE" "$@"
